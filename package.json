{
  "name": "next-prisma",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "npx patch-package",
    "graphql:let": "graphql-let --config .graphql-let.yml -r dotenv/config",
    "graphql:codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "generate:secret": "openssl rand -hex 32",
    "seed": "ts-node src/seeds/seed.ts",
    "clear": "ts-node src/seeds/clear.ts",
    "dev": "next dev -p ${PORT:=3040}",
    "vercel:dev": "vercel dev -p ${PORT:=3040}",
    "dev:debug": "NODE_OPTIONS='--inspect' next -p ${PORT:=3040}",
    "build": "yarn graphql:let && next build",
    "start": "next start -p ${PORT:=3040}",
    "prod:build": "yarn graphql:let && yarn build",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "npx prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api/graphql",
    "docker:build:webapp": "docker build -t nextjs-docker .",
    "docker:run:webapp": "docker run -p 3040:3040 nextjs-docker",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:size": "cross-env ANALYZE=1 next build && cat .next/stats.txt",
    "find:unused": "next-unused",
    "tsconfig:effective": "tsc --showConfig",
    "pretty:quick": "yarn pretty-quick --staged",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky install"
  },
  "next-unused": {
    "alias": {
      "@/apollo/*": [
        "src/apollo/*"
      ],
      "@/cache/*": [
        "src/.cache/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/context/*": [
        "src/context/*"
      ],
      "@/graphql/*": [
        "src/graphql/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/scripts/*": [
        "src/scripts/*"
      ],
      "@/server/*": [
        "src/server/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ]
    },
    "debug": true,
    "include": [
      "src/apollo",
      "src/components",
      "src/graphql",
      "src/lib",
      "src/pages",
      "src/server",
      "src/styles",
      "src/types",
      "src/utils"
    ],
    "exclude": [
      "node_modules"
    ],
    "entrypoints": [
      "src/pages"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@devoxa/prisma-relay-cursor-connection": "^2.1.0",
    "@graphql-tools/graphql-file-loader": "^7.3.4",
    "@graphql-tools/load": "^7.5.2",
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.3",
    "@graphql-tools/schema": "^8.3.2",
    "@graphql-tools/url-loader": "^7.7.2",
    "@graphql-tools/utils": "^8.6.2",
    "@graphql-tools/wrap": "^8.4.2",
    "@headlessui/react": "^1.5.0",
    "@next-auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^3.10.0",
    "@react-spring/web": "^9.4.3",
    "apollo-datasource-http": "^0.19.0",
    "apollo-server-micro": "^3.6.3",
    "apollo-upload-client": "^17.0.0",
    "autoprefixer": "^10.4.2",
    "bcrypt": "^5.0.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "bson": "^4.6.1",
    "classnames": "^2.3.1",
    "cookie": "^0.4.2",
    "cookies-next": "^2.0.4",
    "cross-undici-fetch": "^0.1.25",
    "date-fns": "^2.28.0",
    "debounce": "^1.2.1",
    "express-graphql": "^0.12.0",
    "faker": "^5.5.3",
    "graphql": "^15.8.0",
    "graphql-let": "^0.18.6",
    "graphql-query-complexity": "^0.11.0",
    "graphql-relay": "^0.9.0",
    "graphql-scalars": "^1.14.1",
    "graphql-upload": "^13.0.0",
    "html-react-parser": "^1.4.8",
    "jose": "^4.5.1",
    "jsonwebtoken": "^8.5.1",
    "keen-slider": "^6.6.3",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "mongodb": "^4.4.0",
    "nanoid": "^3.3.1",
    "next": "^12.1.0",
    "next-auth": "^4.2.1",
    "next-rpc": "^3.4.0",
    "next-seo": "^5.1.0",
    "next-superjson": "^0.0.2",
    "next-themes": "^0.1.1",
    "nexus": "^1.2.0",
    "postcss": "^8.4.7",
    "postcss-import": "^14.0.2",
    "prisma-graphql-type-decimal": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.33.1",
    "react-merge-refs": "^1.1.0",
    "react-use-measure": "^2.1.1",
    "superjson": "^1.8.1",
    "swr": "^1.2.2",
    "tabbable": "^5.2.1",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/import-types-preset": "^2.1.12",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/named-operations-object": "^2.2.1",
    "@graphql-codegen/plugin-helpers": "^2.4.1",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typed-document-node": "^2.2.5",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.12",
    "@graphql-codegen/typescript-document-nodes": "^2.2.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@graphql-codegen/typescript-resolvers": "^2.5.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/cookie": "^0.4.1",
    "@types/faker": "^5.5.9",
    "@types/google.analytics": "^0.0.42",
    "@types/graphql-fields": "^1.3.4",
    "@types/graphql-upload": "^8.0.11",
    "@types/gtag.js": "^0.0.8",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.random": "^3.2.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/micro": "^7.3.6",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.9",
    "@types/webpack": "^5.28.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^8.9.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-yml": "^0.14.0",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-createspyobj": "^2.0.0",
    "lint-staged": "^12.3.4",
    "next-sitemap": "^2.5.1",
    "next-unused": "^0.0.6",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.10.0",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended": "^7.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.6.2",
    "utility-types": "^3.10.0",
    "webpack": "^5.69.1",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "yaml-loader": "^0.6.0"
  }
}
